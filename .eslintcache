=> {\r\n                  return (\r\n                    <Table.Row>\r\n                      <Table.Cell width={2}>\r\n                        <Header as=\"h2\" textAlign=\"center\">\r\n                          <span className=\"post-header\">{post.votes}</span>\r\n                        </Header>\r\n                      </Table.Cell>\r\n\r\n                      <Table.Cell>\r\n                        <Header as=\"h3\">\r\n                          <Header.Content>\r\n                            <Link to={`/forum/post/${post.id}`}>\r\n                              <span className=\"post-header\">{post.title}</span>\r\n                            </Link>\r\n                            <Header.Subheader>\r\n                              Date Posted :\r\n                              {moment(post.date).format(\"DD/MM/YYYY\")}\r\n                            </Header.Subheader>\r\n                          </Header.Content>\r\n                        </Header>\r\n                      </Table.Cell>\r\n\r\n                      <Table.Cell width={4}>\r\n                        <Header as=\"h4\" textAlign=\"center\">\r\n                          <span className=\"post-header\">{post.author}</span>\r\n                        </Header>\r\n                      </Table.Cell>\r\n                    </Table.Row>\r\n                  );\r\n                })}\r\n              </Table.Body>\r\n            ) : (\r\n              <Table.Body>\r\n                {data.map((post) => {\r\n                  return (\r\n                    <Table.Row key={post.id}>\r\n                      <Table.Cell width={2}>\r\n                        <Header as=\"h2\" textAlign=\"center\">\r\n                          <span className=\"post-header\">{post.votes}</span>\r\n                        </Header>\r\n                      </Table.Cell>\r\n                      <Table.Cell>\r\n                        <Header as=\"h3\">\r\n                          <Header.Content>\r\n                  